{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": [
      "standard-with-typescript",
      "plugin:prettier/recommended",
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module",
      "project": "./tsconfig.json"
    },
    "rules": {
      "quotes": ["error", "double"],
      "semi": ["error", "always"],
      "prettier/prettier": "off",
      "@typescript-eslint/no-unsafe-argument": "off",
      "@typescript-eslint/explicit-function-return-type":"off"
    }
  }
  